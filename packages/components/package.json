{
  "name": "@quri/squiggle-components",
  "version": "0.7.0",
  "license": "MIT",
  "dependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^3.1.0",
    "@quri/prettier-plugin-squiggle": "workspace:*",
    "@quri/squiggle-lang": "workspace:*",
    "@quri/ui": "workspace:*",
    "@react-hook/size": "^2.1.2",
    "@types/d3": "^7.4.0",
    "clsx": "^1.2.1",
    "codemirror": "^6.0.1",
    "d3": "^7.8.4",
    "framer-motion": "^10.12.16",
    "lodash": "^4.17.21",
    "prettier": "^3.0.0-alpha.12",
    "react": "^18.1.0",
    "react-hook-form": "^7.44.3",
    "react-resizable": "^3.0.5",
    "vscode-uri": "^3.0.7",
    "yup": "^1.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@lezer/generator": "^1.2.3",
    "@storybook/addon-actions": "^7.0.18",
    "@storybook/addon-docs": "^7.0.18",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/blocks": "^7.0.11",
    "@storybook/node-logger": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "canvas": "^2.11.2",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.24",
    "postcss-cli": "^10.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "storybook": "^7.0.18",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "web-vitals": "^3.3.2",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  },
  "scripts": {
    "start": "yarn dev",
    "dev": "storybook dev -p 6006",
    "build:typescript": "rm -rf dist/src dist/tsconfig.tsbuildinfo && tsc",
    "build:css": "postcss ./src/styles/full.css -o ./dist/full.css && postcss ./src/styles/common.css -o ./dist/common.css",
    "build:lezer": "cd ./src/languageSupport; mkdir -p generated; lezer-generator ./squiggle.grammar --output generated/squiggle.ts",
    "build:storybook": "storybook build",
    "build": "yarn run build:lezer && yarn run build:typescript && yarn run build:css && yarn run build:storybook",
    "bundle": "webpack",
    "all": "yarn bundle && yarn build",
    "lint": "prettier --check .",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "format": "prettier --write .",
    "prepack": "yarn run build:typescript && yarn run bundle",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:debug": "node --inspect-brk --experimental-vm-modules node_modules/.bin/jest --runInBand",
    "test:profile": "node --cpu-prof --experimental-vm-modules node_modules/.bin/jest --runInBand"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*",
          "src/index.ts"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./full.css": {
      "default": "./dist/full.css"
    },
    "./common.css": {
      "default": "./dist/common.css"
    },
    "./tailwind-plugin": {
      "default": "./dist/src/tailwind-plugin.cjs"
    }
  },
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "source": "./src/index.ts"
}
