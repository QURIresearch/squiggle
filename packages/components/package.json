    "@hookform/resolvers": "^2.9.11",
    "@quri/squiggle-lang": "^0.6.0",
    "@react-hook/size": "^2.1.2",
    "@types/d3": "^7.4.0",
    "clsx": "^1.2.1",
    "codemirror": "^6.0.1",
    "d3": "^7.8.2",
    "framer-motion": "^10.6.1",
    "lodash": "^4.17.21",
    "react": "^18.1.0",
    "react-hook-form": "^7.43.3",
    "react-use": "^17.4.0",
    "uuid": "^9.0.0",
    "vscode-uri": "^3.0.7",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@lezer/generator": "^1.1.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.14.2",
    "@types/react": "^18.0.21",
    "@types/uuid": "^9.0.1",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "canvas": "^2.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^21.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.1",
    "postcss-nesting": "^11.2.1",
    "react": "^18.1.0",
    "react-scripts": "^5.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.5",
    "web-vitals": "^3.3.0",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18",
    "react-dom": "^16.8.0 || ^17 || ^18"
  },
  "scripts": {
    "start": "cross-env REACT_APP_FAST_REFRESH=false && start-storybook -p 6006 -s public",
    "build:cjs": "rm -rf dist/src && rm -f dist/tsconfig.tsbuildinfo && tsc -b",
    "build:css": "postcss ./src/styles/main.css -o ./dist/main.css",
    "build:lezer": "cd ./src/languageSupport; mkdir -p generated; lezer-generator ./squiggle.grammar --output generated/squiggle.ts",
    "build:storybook": "build-storybook -s public",
    "build": "yarn run build:lezer && yarn run build:cjs && yarn run build:css && yarn run build:storybook",
    "bundle": "webpack",
    "all": "yarn bundle && yarn build",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "format": "prettier --write .",
    "prepack": "yarn run build:cjs && yarn run bundle",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:profile": "node --cpu-prof node_modules/.bin/jest --runInBand"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*",
          "src/index.ts"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "source": "./src/index.ts",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts"
}
