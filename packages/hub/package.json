    "invariant": "^2.2.4",
    "lodash": "^4.17.21",
    "next": "^13.5.3",
    "next-auth": "^4.23.2",
    "nodemailer": "^6.9.5",
    "pako": "^2.1.0",
    "react": "^18.2.0",
