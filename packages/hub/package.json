    "relay": "relay-compiler",
    "print-schema": "tsx ./src/graphql/print-schema.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.6",
    "@pothos/core": "^3.29.0",
    "@pothos/plugin-errors": "^3.11.1",
