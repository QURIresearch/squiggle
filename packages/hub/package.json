    "@types/d3": "^7.4.2",
    "@types/invariant": "^2.2.36",
    "@types/jest": "^29.5.7",
    "@types/lodash": "^4.14.199",
    "@types/pako": "^2.0.1",
    "@types/react": "^18.2.33",
    "@types/react-relay": "^14.1.6",
