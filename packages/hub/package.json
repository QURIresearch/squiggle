{
  "name": "@quri/hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:migrate": "PRISMA_HIDE_UPDATE_MESSAGE=1 prisma migrate deploy",
    "dev": "next dev -p 3001",
    "start": "__NEXT_PRIVATE_PREBUNDLED_REACT=next next start",
    "gen:prisma": "PRISMA_HIDE_UPDATE_MESSAGE=1 prisma generate",
    "gen:relay": "relay-compiler",
    "gen:schema": "tsx ./src/graphql/print-schema.ts",
    "gen": "pnpm gen:prisma && pnpm gen:schema && pnpm gen:relay",
    "build:ts": "pnpm gen && tsc",
    "build": "pnpm gen && __NEXT_PRIVATE_PREBUNDLED_REACT=next next build",
    "lint": "prettier --check . && next lint",
    "format": "prettier --write .",
    "test:manual": "dotenv -e .env.test -- jest -i"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@pothos/core": "^3.39.0",
    "@pothos/plugin-errors": "^3.11.1",
    "@pothos/plugin-prisma": "^3.63.0",
    "@pothos/plugin-relay": "^3.45.1",
    "@pothos/plugin-scope-auth": "^3.20.0",
    "@pothos/plugin-simple-objects": "^3.7.0",
    "@pothos/plugin-validation": "^3.10.1",
    "@pothos/plugin-with-input": "^3.10.1",
    "@prisma/client": "5.7.1",
    "@quri/squiggle-components": "workspace:*",
    "@quri/squiggle-lang": "workspace:*",
    "@quri/ui": "workspace:*",
    "@quri/versioned-squiggle-components": "workspace:*",
    "base64-js": "^1.5.1",
    "clsx": "^2.1.0",
    "d3": "^7.8.5",
    "date-fns": "^3.3.1",
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.1.0",
    "immutable": "^4.3.4",
    "invariant": "^2.2.4",
    "lodash": "^4.17.21",
    "next": "^14.0.4",
    "next-auth": "^4.24.4",
    "nodemailer": "^6.9.8",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.0",
    "react-icons": "^5.0.1",
    "react-loading-skeleton": "^3.4.0",
    "react-markdown": "^9.0.1",
    "react-relay": "^16.1.0",
    "react-select": "^5.7.7",
    "relay-runtime": "^16.2.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@quri/configs": "workspace:*",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.4.0",
    "@types/d3": "^7.4.3",
    "@types/invariant": "^2.2.37",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.2.46",
    "@types/react-relay": "^16.0.4",
    "@types/relay-runtime": "^14.1.19",
    "babel-plugin-relay": "^16.1.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.3",
    "graphql": "^16.8.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
