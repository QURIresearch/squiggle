    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.2.6",
    "@types/react-relay": "^14.1.3",
    "@types/relay-runtime": "^14.1.10",
    "babel-plugin-relay": "^15.0.0",
    "eslint-config-next": "^13.2.4",
    "graphql": "^16.6.0",
    "postcss": "^8.4.21",
    "prisma": "^4.14.0",
