{
  "name": "@quri/hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:generate": "prisma generate",
    "dev": "next dev -p 3001",
    "start": "__NEXT_PRIVATE_PREBUNDLED_REACT=next next start",
