{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {},
    "format": {},
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "bundle": {
      "dependsOn": ["^build", "build"]
    },
    "vscode-squiggle#build": {
      "dependsOn": ["^build", "@quri/squiggle-components#bundle"]
    },
    "coverage": {
      "dependsOn": ["build"]
    }
  }
}
